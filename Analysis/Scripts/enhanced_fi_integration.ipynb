   "source": [
    "print(\"=== ISIN INTEGRATION FOR FUTURE DATA SOURCING ===\")\n",
    "\n",
    "# Create ISIN reference table for future data collection\n",
    "isin_reference = enhanced_df[['Bank', 'Country', 'ISIN', 'Mechanism']].copy()\n",
    "isin_reference = isin_reference.sort_values(['Country', 'Bank'])\n",
    "\n",
    "print(\"\\nISIN Reference Table (for data sourcing):\")\n",
    "current_country = None\n",
    "for _, row in isin_reference.iterrows():\n",
    "    if row['Country'] != current_country:\n",
    "        current_country = row['Country']\n",
    "        print(f\"\\n{current_country}:\")\n",
    "    print(f\"  {row['Bank']}: {row['ISIN']}\")\n",
    "\n",
    "# Export ISIN reference\n",
    "isin_reference.to_csv('../results/isin_reference_table.csv', index=False)\n",
    "\n",
    "# Create data collection template\n",
    "data_template = enhanced_df[[\n",
    "    'Bank', 'Country', 'ISIN', 'Mechanism',\n",
    "    'Green_Finance_B_USD', 'Fossil_Finance_B_USD', \n",
    "    'SBTi_Target', 'NZBA_Member'\n",
    ]].copy()\n",
    "\n",
    "# Add empty columns for additional data collection\n",
    "data_template['Total_Assets_B_USD'] = np.nan\n",
    "data_template['Financed_Emissions_MtCO2'] = np.nan\n",
    "data_template['Green_Bond_Issuance_B_USD'] = np.nan\n",
    "data_template['SLL_Issuance_B_USD'] = np.nan\n",
    "data_template['TCFD_Compliant'] = np.nan\n",
    "data_template['PRB_Signatory'] = np.nan\n",
    "data_template['Data_Source'] = ''\n",
    "data_template['Last_Updated'] = ''\n",
    "\n",
    "data_template.to_csv('../results/fi_data_collection_template.csv', index=False)\n",
    "\n",
    "print(f\"\\n✓ ISIN reference table saved to results/isin_reference_table.csv\")\n",
    "print(f\"✓ Data collection template saved to results/fi_data_collection_template.csv\")\n",
    "print(f\"\\nUse these files for:\")\n",
    "print(f\"• Automated data collection via financial APIs\")\n",
    "print(f\"• ESG database queries\")\n",
    "print(f\"• Manual data verification and updates\")\n",
    "print(f\"• Thesis appendix reference tables\")"
