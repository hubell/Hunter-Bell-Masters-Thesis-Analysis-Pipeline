    "## Section 6: Updated Key Findings with Expanded Coverage"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"=== UPDATED KEY FINDINGS WITH ENHANCED COVERAGE ===\")\n",
    "\n",
    "# Recalculate key metrics with full coverage\n",
    "chinese_banks = enhanced_df[enhanced_df['Region'] == 'Chinese Banks']\n",
    "nzba_banks = enhanced_df[enhanced_df['NZBA_Member'] == True]\n",
    "us_banks = enhanced_df[enhanced_df['Region'] == 'US Banks (NZBA Members)']\n",
    "brazilian_banks = enhanced_df[enhanced_df['Region'] == 'Brazilian Banks']\n",
    "indian_banks = enhanced_df[enhanced_df['Region'] == 'Indian Banks']\n",
    "\n",
    "print(\"\\n1. IMPLEMENTATION WITHOUT ADOPTION (Updated):\")\n",
    "total_chinese_green = chinese_banks['Green_Finance_B_USD'].sum()\n",
    "chinese_framework_adoption = chinese_banks['SBTi_Target'].sum() + chinese_banks['NZBA_Member'].sum()\n",
    "print(f\"   • Chinese Big Four: ${total_chinese_green}B green credit, {chinese_framework_adoption} framework adoptions\")\n",
    "print(f\"   • Average Paradox Ratio: {chinese_banks['Paradox_Ratio'].mean():.1f}:1\")\n",
    "\n",
    "print(\"\\n2. NZBA PARADOX (Comprehensive):\")\n",
    "total_nzba_fossil = nzba_banks['Fossil_Finance_B_USD'].sum()\n",
    "total_nzba_green = nzba_banks['Green_Finance_B_USD'].sum()\n",
    "print(f\"   • {len(nzba_banks)} NZBA members analyzed\")\n",
    "print(f\"   • Total Fossil Finance: ${total_nzba_fossil}B\")\n",
    "print(f\"   • Total Green Finance: ${total_nzba_green}B\")\n",
    "print(f\"   • Fossil-to-Green Ratio: {total_nzba_fossil/total_nzba_green:.1f}:1\")\n",
    "\n",
    "print(\"\\n3. REGIONAL IMPLEMENTATION EFFECTIVENESS:\")\n",
    "# Calculate by regulatory mechanism\n",
    "directive_banks = enhanced_df[enhanced_df['Mechanism'].str.contains('CERF|Priority|Environmental', na=False)]\n",
    "market_banks = enhanced_df[enhanced_df['Mechanism'].str.contains('Voluntary|EU Taxonomy', na=False)]\n",
    "\n",
    "directive_ies = directive_banks['IES_Score'].mean()\n",
    "market_ies = market_banks['IES_Score'].mean()\n",
    "implementation_gap = directive_ies - market_ies\n",
    "\n",
    "print(f\"   • Directive Mechanisms: {directive_ies:.1f}% average IES\")\n",
    "print(f\"   • Market-Based Mechanisms: {market_ies:.1f}% average IES\") \n",
    "print(f\"   • Implementation Gap: {implementation_gap:.1f} percentage points\")\n",
    "\n",
    "print(\"\\n4. FRAMEWORK CORRELATION (Updated):\")\n",
    "framework_score_all = (enhanced_df['SBTi_Target'].astype(int) + enhanced_df['NZBA_Member'].astype(int)) / 2\n",
    "correlation_all = np.corrcoef(framework_score_all, enhanced_df['IES_Score'])[0,1]\n",
    "print(f\"   • Framework Adoption ↔ Implementation: r = {correlation_all:.3f}\")\n",
    "print(f\"   • Relationship: {'Positive' if correlation_all > 0.1 else 'Near Zero' if abs(correlation_all) < 0.1 else 'Negative'}\")\n",
    "\n",
    "print(\"\\n5. TOP PERFORMERS BY PARADOX RATIO:\")\n",
    "top_5_paradox = enhanced_df.nlargest(5, 'Paradox_Ratio')\n",
    "for _, bank in top_5_paradox.iterrows():\n",
    "    print(f\"   • {bank['Bank']} ({bank['Country']}): {bank['Paradox_Ratio']:.1f}:1\")\n",
    "\n",
    "print(\"\\n6. COMPREHENSIVE COVERAGE ACHIEVED:\")\n",
    "print(f\"   • Total Banks: {len(enhanced_df)}\")\n",
    "print(f\"   • Countries: {enhanced_df['Country'].nunique()}\")\n",
    "print(f\"   • Total Green Finance: ${enhanced_df['Green_Finance_B_USD'].sum():.1f}B\")\n",
    "print(f\"   • Regulatory Mechanisms: {enhanced_df['Mechanism'].nunique()}\")\n",
    "\n",
    "# Export comprehensive results\n",
    "enhanced_df.to_csv('../results/comprehensive_fi_analysis.csv', index=False)\n",
    "print(f\"\\n✓ Comprehensive results exported to results/comprehensive_fi_analysis.csv\")\n",
    "print(f\"✓ Ready for integration into thesis Chapters 4-6\")"
   ]
